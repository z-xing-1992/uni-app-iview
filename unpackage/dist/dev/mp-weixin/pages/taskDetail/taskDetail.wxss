
page{width: 100%;height: 100%;}
.taskDetail-container{width: 100%;height: 100%;background: #F6F5F5;position: relative;}
.title{width: 100%;overflow: hidden;background: #3E414F;}
.title image{display: block;width: 80rpx;height: 72rpx;float: left;margin: 20rpx 21rpx 75rpx 192rpx;}
.title text{display: block;float: left;font-size:38rpx;color: #fff;margin-top: 30rpx;}
.warning{width: 100%;overflow: hidden;background: #3E414F;}
.warning image{display: block;width: 80rpx;height: 72rpx;float: left;margin: 20rpx 21rpx 75rpx 136rpx;}
.warning text{display: block;float: left;font-size:38rpx;color: #fff;margin-top: 30rpx;}
.boxes{width: 100%;overflow: hidden;position: absolute;left: 0;top: 130rpx;}
.main{width:702rpx;overflow: hidden;margin: 0 auto;-webkit-border-radius:8rpx;border-radius:8rpx;background: #fff;padding: 30rpx 27rpx;}
.Id-Name{display: -webkit-box;display: -webkit-flex;display: flex;-webkit-box-pack: justify;-webkit-justify-content: space-between;justify-content: space-between;}
.id text{display: inline-block;}
.id text:nth-of-type(1),.name text:nth-of-type(1){font-size:26rpx;color: #777777;}
.id text:nth-of-type(2),.name text:nth-of-type(2){font-size: 26rpx;color: #3E414F;font-weight: bold;}
.time {display: -webkit-box;display: -webkit-flex;display: flex;-webkit-box-pack: justify;-webkit-justify-content: space-between;justify-content: space-between;margin-top: 28rpx;border-bottom: 1rpx solid #EDEDED;}
.time view text{display: block;}
.time view:nth-of-type(2) image{width: 54rpx;height: 54rpx;display: block;}
.begin-time text:nth-of-type(1),.delivery-time text:nth-of-type(1){font-size:26rpx;color: #777777;}
.begin-time text:nth-of-type(2){color:#3E414F;font-weight: bold;margin-top: 16rpx;margin-bottom: 35rpx;}
.delivery-time text:nth-of-type(2){color:#FF8833;font-weight: bold;margin-top: 16rpx;margin-bottom: 35rpx;}
.number-materials{width: 100%;overflow: hidden;margin-top: 28rpx;display: -webkit-box;display: -webkit-flex;display: flex;-webkit-box-pack: justify;-webkit-justify-content: space-between;justify-content: space-between;
	border-bottom: 1rpx solid #EDEDED;}
.number{width: 230rpx;overflow: hidden;text-align: center;}
.number-detail image{width: 184rpx;height: 184rpx;}
.cloud{font-size: 30rpx;color: #3E414F;font-weight: bold;}
.number-detail{margin:29rpx 0 20rpx 0;position: relative;}
.number-detail-num{position: absolute;width: 100%;height: 100%;top: 0;left: 0;}
.number-detail-num view{margin: 46rpx 0 0 0;}
.number-detail-num view text{font-size: 50rpx;font-weight: bold;}
.number-detail-num view text:nth-of-type(1){color: #FFFFFF;}
.number-detail-num view text:nth-of-type(2){color: #E7E7E7;}
.line-rolad text:nth-of-type(1){font-size: 26rpx;color: #777777;}
.line-rolad text:nth-of-type(2){font-size: 26rpx;color: #3E414F;font-weight: bold;}
.materials{text-align: right;}
.materials text{display: inline-block;}
.materials view{margin-bottom: 29rpx;}
.materials view:nth-of-type(1){font-size: 30rpx;color: #3E414F;font-weight: bold;}
.materials view text:nth-of-type(1){font-size: 26rpx;color: #3E414F;}
.materials view text:nth-of-type(2){width: 40rpx;height: 40rpx;background: #3BBCA3;-webkit-border-radius: 50%;border-radius: 50%;font-size: 24rpx;color: #fff;text-align: center;line-height: 40rpx;margin-left: 21rpx;}
.goods{width: 100%;overflow: hidden;}
.goods-title{width: 100%;font-size: 30rpx;color: #3E414F;font-weight: bold;margin: 29rpx 0 19rpx 0;}
.goods-detail{width: 100%;min-height: 158rpx;background: #F6F5F5;-webkit-border-radius: 8rpx;border-radius: 8rpx;font-size: 28rpx;color: #494C5D;padding: 26rpx 27rpx 26rpx 27rpx;}
.bottom{width: 100%;height: 120rpx;background: #fff;display: -webkit-box;display: -webkit-flex;display: flex;-webkit-box-pack: space-evenly;-webkit-justify-content: space-evenly;justify-content: space-evenly;position: fixed;bottom: 0;left: 0;}
.bottom view image{display: block;width: 40rpx;height: 38rpx;}
.taking-pictures{width: 150rpx;}
.taking-pictures image{margin: 18rpx auto 10rpx;}
.taking-pictures text{display: block;text-align: center;font-size: 26rpx;color: #3E414F;}
.look {text-align: center;}
.look text{display: inline-block;width: 260rpx;height: 90rpx;line-height: 90rpx;font-size: 30rpx;color: #3E414F;
border: 2rpx solid #C5C9D1;-webkit-border-radius: 8rpx;border-radius: 8rpx;text-align: center;margin: 15rpx 0 15rpx 0;}
.sure text{display: inline-block;width: 260rpx;height: 90rpx;line-height: 90rpx;font-size: 30rpx;color: #fff;	
	background: -webkit-gradient(linear, right top, left top, from(#FF8833), to(#FAA951));	
	background: -webkit-linear-gradient(right, #FF8833 0%, #FAA951 100%);	
	background: linear-gradient(270deg, #FF8833 0%, #FAA951 100%);-webkit-border-radius: 8rpx;border-radius: 8rpx;text-align: center;margin: 15rpx 0 15rpx 0;}
.back text{display: inline-block;width: 412rpx;height: 90rpx;line-height: 90rpx;font-size: 30rpx;color: #fff;
	background: -webkit-gradient(linear, right top, left top, from(#FF8833), to(#FAA951));
	background: -webkit-linear-gradient(right, #FF8833 0%, #FAA951 100%);
	background: linear-gradient(270deg, #FF8833 0%, #FAA951 100%);-webkit-border-radius: 8rpx;border-radius: 8rpx;text-align: center;margin: 15rpx 0 15rpx 0;}
.zhanwei{width: 100%;height: 150rpx;background: #F6F5F5}
.pictureList image{width: 203rpx;height: 176rpx;display: block;float: left;margin-right: 15rpx;margin-top: 10rpx;}
.pictureList text{display: block;width: 100%;height: 158rpx;background: #F6F5F5;-webkit-border-radius: 8rpx;border-radius: 8rpx;padding: 26rpx;font-size: 28rpx;color: #494C5D;}

