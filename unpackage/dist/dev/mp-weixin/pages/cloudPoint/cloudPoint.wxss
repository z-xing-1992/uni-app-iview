
page {
	width: 100%;
	height: 100%;
}
.cloudPoint-container {
	width: 100%;
	height: 100%;
	position: relative;
}
.header-zhan{width: 100%;height: 80rpx;}
.state-switch {
	width: 100%;
	overflow: hidden;
	position: fixed;
	top: 0;
	left: 0;
	z-index: 999;
}
.cloudPoint-list {
	width: 100%;
	overflow: hidden;
	background: #F6F5F5;
	padding-top: 20rpx;
}
.backUp {
	width: 100%;
	height: 90rpx;
	font-size: 30rpx;
	color: #fff;
	text-align: center;
	line-height: 90rpx;
	background: -webkit-gradient(linear, right top, left top, from(#FF8833), to(#FAA951));
	background: -webkit-linear-gradient(right, #FF8833 0%, #FAA951 100%);
	background: linear-gradient(270deg, #FF8833 0%, #FAA951 100%);
	position: fixed;
	left: 0;
	bottom: 0;
}
.loudPoint-zhanwei{width: 100%;height:120rpx;}

