
page{width: 100%;height: 100%;}
.cloudDetail-container{width: 100%;height: 100%;position: relative;background: #F6F5F5}
.title{width: 100%;overflow: hidden;background: #3E414F;}
.title image{display: block;width: 80rpx;height: 72rpx;float: left;margin: 20rpx 21rpx 75rpx 192rpx;}
.title text{display: block;float: left;font-size:38rpx;color: #fff;margin-top: 30rpx;}
.cloud-boxes{width: 100%;overflow: hidden;position: absolute;left: 0;top: 130rpx;}
.cloud-card{width:702rpx;overflow: hidden;margin: 0 auto;-webkit-border-radius:8rpx;border-radius:8rpx;background: #fff;padding: 30rpx 27rpx;}
.Id-Name,.Code-Order{display: -webkit-box;display: -webkit-flex;display: flex;-webkit-box-pack: justify;-webkit-justify-content: space-between;justify-content: space-between;}
.id text,.cloud-order text,.Delivery-time{display: inline-block;}
.id text:nth-of-type(1),.name text:nth-of-type(1),.cloud-order text:nth-of-type(1),.cloud-code text:nth-of-type(1),.Delivery-time text:nth-of-type(1){font-size:26rpx;color: #777777;}
.id text:nth-of-type(2),.name text:nth-of-type(2),.cloud-order text:nth-of-type(2),.cloud-code text:nth-of-type(2){font-size: 26rpx;color: #3E414F;font-weight: bold;}
.Delivery-time text:nth-of-type(2){font-size: 26rpx;color: #FF8833;}
.Code-Order{margin-top: 30rpx;}
.Delivery-time{margin: 30rpx 0 10rpx 0;}
.cloud-navigation{width: 100%;overflow: hidden;border-top:2rpx solid #EDEDED;margin-top: 20rpx;}
.cloud-loction-detail{float: left;width: 480rpx;overflow: hidden;font-size: 30rpx;color: #3E414F;margin-top: 20rpx;}
.cloud-loction-detail text:nth-of-type(1){font-size: 30rpx;color: #777777;}
.cloud-map{float: right;width: 164rpx;height: 124rpx;}
.cloud-map image{width: 164rpx;height: 124rpx;margin-top: 20rpx;}
.cloud-main{width: 100%;overflow: hidden;padding: 20rpx 24rpx 0 24rpx;margin-top:40rpx;background: #fff;}
.door-photo{width: 100%;overflow: hidden;}
.door-photo text{display: inline-block;font-size: 30rpx;color: #3E414F;font-weight: bold;}
.lunbo{width: 100%;overflow: hidden;margin-top: 19rpx;}
.all-goods{width: 100%;overflow: hidden;margin-top: 40rpx;}
.all-goods view:nth-of-type(1){font-size: 30rpx;color: #3E414F;font-weight: bold;float: left;}
.all-goods view:nth-of-type(2){float: left;}
.all-goods view:nth-of-type(2) text{display: block;float: left;}
.all-goods view:nth-of-type(2) text:nth-of-type(1),.all-goods view:nth-of-type(2) text:nth-of-type(3),.all-goods view:nth-of-type(2) text:nth-of-type(5),.all-goods view:nth-of-type(2) text:nth-of-type(7),.all-goods view:nth-of-type(2) text:nth-of-type(9){
	font-size: 26rpx;color: #3E414F;}
.all-goods view:nth-of-type(2) text:nth-of-type(2){width: 26rpx;height: 26rpx;background: #FF5959;margin: 5rpx 7rpx 0 7rpx;}
.all-goods view:nth-of-type(2) text:nth-of-type(4){width: 26rpx;height: 26rpx;background: #06BD7D;margin: 5rpx 7rpx 0 7rpx;}
.all-goods view:nth-of-type(2) text:nth-of-type(6){width: 26rpx;height: 26rpx;background: #4868FB;margin: 5rpx 7rpx 0 7rpx;}
.all-goods view:nth-of-type(2) text:nth-of-type(8){width: 26rpx;height: 26rpx;background: #F6F5F5;margin: 5rpx 7rpx 0 7rpx;}
.all-goods-table{margin-top: 19rpx;}
.cloud-special-goods{width: 100%;overflow: hidden;background: #F6F5F5;-webkit-border-radius: 8rpx;border-radius: 8rpx;min-height: 158rpx;height: auto;
padding: 32rpx 26rpx;margin-top: 19rpx;font-size: 28rpx;color: #494C5D;}
.cloud-zhanwei{width: 100%;height: 200rpx;}
.cloud-bootom{width: 100%;height: 120rpx;background: #fff;-webkit-box-shadow: 0px 0px 14px 0px rgba(4, 0, 0, 0.1);box-shadow: 0px 0px 14px 0px rgba(4, 0, 0, 0.1);position: fixed;left: 0;bottom: 0;
display: -webkit-box;
display: -webkit-flex;
display: flex;-webkit-box-pack: space-evenly;-webkit-justify-content: space-evenly;justify-content: space-evenly;}
.cloud-bootom view image{display: block;width: 40rpx;height: 38rpx;}
.taking-pictures{width: 150rpx;}
.taking-pictures image{margin: 18rpx auto 10rpx;}
.taking-pictures text{display: block;text-align: center;font-size: 26rpx;color: #3E414F;}
.look {text-align: center;}
.look text{display: inline-block;width: 260rpx;height: 90rpx;line-height: 90rpx;font-size: 30rpx;color: #3E414F;
border: 2rpx solid #C5C9D1;-webkit-border-radius: 8rpx;border-radius: 8rpx;text-align: center;margin: 15rpx 0 15rpx 0;}
.sure text{display: inline-block;width: 260rpx;height: 90rpx;line-height: 90rpx;font-size: 30rpx;color: #fff;	
	background: -webkit-gradient(linear, right top, left top, from(#FF8833), to(#FAA951));	
	background: -webkit-linear-gradient(right, #FF8833 0%, #FAA951 100%);	
	background: linear-gradient(270deg, #FF8833 0%, #FAA951 100%);-webkit-border-radius: 8rpx;border-radius: 8rpx;text-align: center;margin: 15rpx 0 15rpx 0;}
.back text{display: inline-block;width: 412rpx;height: 90rpx;line-height: 90rpx;font-size: 30rpx;color: #fff;
	background: -webkit-gradient(linear, right top, left top, from(#FF8833), to(#FAA951));
	background: -webkit-linear-gradient(right, #FF8833 0%, #FAA951 100%);
	background: linear-gradient(270deg, #FF8833 0%, #FAA951 100%);-webkit-border-radius: 8rpx;border-radius: 8rpx;text-align: center;margin: 15rpx 0 15rpx 0;}
.goods{width: 100%;overflow: hidden;}
.goods-title{width: 100%;font-size: 30rpx;color: #3E414F;font-weight: bold;margin: 29rpx 0 19rpx 0;}
.goods-detail{width: 100%;min-height: 158rpx;background: #F6F5F5;-webkit-border-radius: 8rpx;border-radius: 8rpx;font-size: 28rpx;color: #494C5D;padding: 26rpx 27rpx 26rpx 27rpx;}
.pictureList image{width: 203rpx;height: 176rpx;display: block;float: left;margin-right: 15rpx;margin-top: 20rpx;}
.pictureList text{display: block;width: 100%;height: 158rpx;background: #F6F5F5;-webkit-border-radius: 8rpx;border-radius: 8rpx;padding: 26rpx;font-size: 28rpx;color: #494C5D;}
.green{background: #06BD7D;}
.acceptancePicture{width: 100%;overflow: hidden;}
.chooseImg{width: 180rpx;height:180rpx;float: left;
	border: 2rpx dashed #C5C9D1;
	-webkit-border-radius: 8rpx;
	        border-radius: 8rpx;margin-left: 20rpx;margin-top: 10rpx;}
.chooseImg image{width: 54rpx;height: 51rpx;display: block;margin: 42rpx auto 19rpx;}
.chooseImg text{display: block;width: 100%;font-size: 26rpx;color:#3E414F;text-align: center;}
.backPhoto{width: 180rpx;height: 180rpx;float: left;margin-left:20rpx;margin-top: 10rpx;-webkit-border-radius: 8rpx;border-radius: 8rpx;}
.backPhoto image{width:180rpx;height:180rpx;-webkit-border-radius: 8rpx;border-radius: 8rpx;}
.abnormality{width: 710rpx;margin: 0 auto;min-height: 600rpx;}
.abnormality-title{width: 100%;overflow: hidden;font-size: 34rpx;text-align: center;margin-top: 20rpx;font-weight: bold;}
.abnormality-reson{width: 100%;overflow: hidden;margin-top: 20rpx;font-weight: bold;font-size: 30rpx;}
.abnormality-reson-item{width: 100%;overflow: hidden;margin-top: 20rpx;}
.abnormality-reson-item text{display: inline-block;min-width:200rpx;height: 66rpx;text-align: center;line-height: 66rpx;
	font-size:26rpx;-webkit-border-radius: 8rpx;border-radius: 8rpx;margin-bottom: 20rpx;margin-right: 10rpx;float: left;}
.affirm{width: 100%;display: -webkit-box;display: -webkit-flex;display: flex;-webkit-box-pack: space-evenly;-webkit-justify-content: space-evenly;justify-content: space-evenly;margin-top: 100rpx;}
.affirm text{display: block;font-size: 30rpx;text-align: center;height: 90rpx;line-height: 90rpx;}
.affirm text:nth-of-type(1){width: 260rpx;border: 2rpx solid #C5C9D1;-webkit-border-radius: 8rpx;border-radius: 8rpx;color: #3E414F;}
.affirm text:nth-of-type(2){width: 412rpx;background: -webkit-gradient(linear, right top, left top, from(#FF8833), to(#FAA951));background: -webkit-linear-gradient(right, #FF8833 0%, #FAA951 100%);background: linear-gradient(270deg, #FF8833 0%, #FAA951 100%);-webkit-border-radius: 8rpx;border-radius: 8rpx;color: #FFFFFF;}
.active{background: #FFF3EA;border: 2rpx solid #FF8833;-webkit-border-radius: 8rpx;border-radius: 8rpx;color: #FF8833;}
.actove{background: #F5F7FA;-webkit-border-radius: 8rpx;border-radius: 8rpx;}
.address-note{width: 100%;overflow: hidden;}
.address-note textarea{width: 100%;height: 188rpx;background: #F6F5F5;-webkit-border-radius: 8rpx;border-radius: 8rpx;padding: 32rpx 26rpx;margin-top: 19rpx;font-size: 28rpx;color: #494C5D;-webkit-box-sizing: border-box;box-sizing: border-box;}

