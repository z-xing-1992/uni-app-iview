<view data-event-opts="{{[['tap',[['cloudDetail',['$event']]]]]}}" class="cloud-item" bindtap="__e"><view class="item-left">{{''+item.shipOrder+''}}</view><view class="item-right"><view class="cloud-code"><text class="_label">团点代码：</text><text class="_label-detail">{{item.consigneeNo}}</text></view><view class="cloud-name"><text class="_label">团点名称：</text><text>{{item.storeName}}</text></view><view class="cloud-name"><text class="_label">团点地址：</text><text>{{item.address}}</text></view><view class="phone-location"><view data-event-opts="{{[['tap',[['tellphone',['$0','$1'],['item.mobilephone','item.telephone']]]]]}}" class="checkphone" catchtap="__e"><image src="../static/icon/boda.png" mode></image><text>拨打</text></view><view data-event-opts="{{[['tap',[['navigation',['$0','$1'],['item.latitude','item.longitude']]]]]}}" class="location-logo" catchtap="__e"><image src="../static/icon/navigation.png" mode></image><text>导航</text></view></view></view><block wx:if="{{item.status=='30交付完成'||item.status=='99放弃'}}"><view class="zhezhao"><block wx:if="{{item.status=='30交付完成'}}"><text>此任务已交付完成</text></block><block wx:if="{{item.status=='99放弃'}}"><text>此任务异常</text></block><text>查看明细</text><block wx:if="{{item.status=='30交付完成'}}"><view class="success"><image src="../static/icon/checkbox-circle.png" mode></image>已交付</view></block><block wx:if="{{item.status=='99放弃'}}"><view class="abnormal"><image src="../static/icon/error-warning-line.png" mode></image>异常</view></block></view></block></view>