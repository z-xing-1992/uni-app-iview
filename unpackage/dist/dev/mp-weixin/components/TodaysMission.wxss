
.TodaysMission-item{width: 702rpx;overflow: hidden;background: #FFFFFF;-webkit-box-shadow: 0px 0px 16px 0px rgba(62, 65, 79, 0.3);box-shadow: 0px 0px 16px 0px rgba(62, 65, 79, 0.3);
-webkit-border-radius: 8px;
        border-radius: 8px;margin: 20rpx auto 20rpx;padding: 0 0 0 27rpx;position: relative;}
.TodaysMission-item text{display: inline-block;font-size: 26rpx;}
.item-top,.item-centre,.item-bottom{display: -webkit-box;display: -webkit-flex;display: flex;-webkit-box-pack: justify;-webkit-justify-content: space-between;justify-content: space-between;}
.item-top{margin-top: 26rpx;width: 100%;}
.item-centre{margin-top: 20rpx;width: 100%;}
.progress{width: 650rpx;border-bottom: 1rpx solid #EDEDED;overflow: hidden;}
.item-bottom{margin:20rpx 0 26rpx 0;width: 100%;}
.line-road{margin-right: 225rpx;}
.progress-detail{margin-right: 215rpx;}
.detail {position: relative;}
.detail text{width: 142rpx;height: 48rpx;background: #FF8833;-webkit-border-radius: 8rpx 0px 0px 8rpx;border-radius: 8rpx 0px 0px 8rpx;line-height: 48rpx;text-indent: 1em;font-size: 30rpx;color: #fff;}
.detail image{width: 11rpx;height: 18rpx;float: right;position: absolute;right: 30rpx;top: 18rpx;}
.complete{float: left;display: inline-block;margin:20rpx 0 0 10rpx;font-size: 26rpx;color: #3E414F;}

